{% extends 'base.html.twig' %}

{% block title %}Welcome!{% endblock %}

{% block body %}

    <div class="container my-3">
        <h1>Welcome to dashboard {{ number }}</h1>

        <div id="user-list">
            <ul>
                {%  for user in users %}
                    <li>{{ user }}</li>
                {%  endfor %}
            </ul>
        </div>
    </div>

{% endblock %}